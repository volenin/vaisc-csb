   "source": [
    "import pandas as pd\n",
    "from IPython.display import display_html\n",
    "\n",
    "max_rows = 5  # Set the maximum number of rows to display\n",
    "\n",
    "# Create side-by-side comparison of products from list_products and search_products\n",
